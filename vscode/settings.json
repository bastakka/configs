{
    "files.autoSave": "afterDelay",
    "git.ignoreMissingGitWarning": true,
    "debug.onTaskErrors": "debugAnyway",
    "explorer.confirmDragAndDrop": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb",
        "*.pdf": "latex-workshop-pdf-hook"
    },
    "editor.fontSize": 12,
    "workbench.colorTheme": "Activate UMBRA protocol",
    "window.autoDetectHighContrast": false,
    "python.autoComplete.addBrackets": true,
    "github.copilot.autocomplete.enable": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": true
    },
    "python.analysis.completeFunctionParens": true,
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "vscode-icons",
    "editor.inlineSuggest.enabled": true,
    "vsicons.dontShowNewVersionMessage": true,
    "python.defaultInterpreterPath": "C:\\Users\\basta\\AppData\\Local\\Microsoft\\WindowsApps\\python3.10.exe",
    "git.confirmSync": false,
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "vscode-office.openOutline": true,
    "[nginx]": {
        "editor.defaultFormatter": "ahmadalli.vscode-nginx-conf"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.editor.untitled.hint": "hidden",
    "latex-workshop.view.pdf.viewer": "browser",
    "window.zoomLevel": 1,
    "terminal.integrated.fontFamily": "JetBrainsMono NF",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.lineHeight": 1
}
